<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXTextField?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.*?>
<AnchorPane prefHeight="100.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="EditHeader">
   <children>
      <VBox prefHeight="100.0" prefWidth="400.0">
         <children>
            <HBox alignment="CENTER_LEFT" prefHeight="70.0" prefWidth="200.0">
               <children>
                  <Label styleClass="labels" text="New header">
                     <HBox.margin>
                        <Insets left="15.0" />
                     </HBox.margin>
                     <padding>
                        <Insets top="15.0" />
                     </padding>
                  </Label>
                  <JFXTextField fx:id="newHeaderTextField" styleClass="highlightedelements">
                     <HBox.margin>
                        <Insets left="10.0" top="13.0" />
                     </HBox.margin>
                  </JFXTextField>
               </children>
            </HBox>
            <JFXButton fx:id="applyButton" onAction="#applyAction" styleClass="raised-buttons" text="Apply">
               <VBox.margin>
                  <Insets bottom="8.0" left="8.0" />
               </VBox.margin>
            </JFXButton>
         </children>
      </VBox>
   </children>
</AnchorPane>
